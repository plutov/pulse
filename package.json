{
  "scripts": {
    "dev": "dotenv -e .env -- ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "oapi": "openapi-generator-cli generate -i openapi.yaml -g typescript-axios -o ./src/apigen -c openapi.config.yaml",
    "db:migrate": "dotenv -e .env -- knex migrate:latest --knexfile knexfile.ts",
    "db:seed": "dotenv -e .env -- knex seed:run --knexfile knexfile.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "types:generate": "dotenv -e .env -- kanel --config .kanelrc.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@testcontainers/postgresql": "^11.6.0",
    "@testcontainers/redis": "^11.6.0",
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "3.6.2",
    "supertest": "^7.1.4",
    "testcontainers": "^11.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/hapi": "^21.4.3",
    "@types/bcrypt": "^6.0.0",
    "@types/hapi": "^18.0.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "hapi-auth-jwt2": "^11.0.0",
    "hapi-pino": "^13.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "kanel": "^3.14.4",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  }
}
